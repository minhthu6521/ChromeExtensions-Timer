{
    "manifest_version": 2,
    "name": "Timer",
    "version": "1.0.0",
    
    "description": "A chrome extension that allows you to set timer and alert",
    "author": "ThuNguyen",
    
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "index.html",
        "default_title": "Set!"
    },
    
    "background":{
        "scripts":["background.js"]
    },
    
    "permissions":[
        "notifications"
    ]
}